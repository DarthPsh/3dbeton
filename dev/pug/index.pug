		.container 
			+section-title({name:'Наши проекты в 35+ городах', link: 'В РАЗДЕЛ ПОРТФОЛИО', href: '#'})
			.section-content.pageslider-content
				.pageslider.swiper-container 
					.swiper-wrapper
						.pageslider-item.swiper-slide
							.pageslider-item__side.pageslider-item__main 
